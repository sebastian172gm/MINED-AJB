                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT PERIODO,
CASE 
WHEN PERIODO = '20196' THEN 2019
WHEN PERIODO = '20184' THEN 2018
WHEN PERIODO = '20194' THEN 2019
WHEN PERIODO = '20212' THEN 2021
WHEN PERIODO = '20223' THEN 2022
WHEN PERIODO = '20183' THEN 2018
WHEN PERIODO = '20203' THEN 2020
WHEN PERIODO = '20195' THEN 2019
WHEN PERIODO = '20222' THEN 2022
WHEN PERIODO = '20225' THEN 2022
WHEN PERIODO = '20226' THEN 2022
WHEN PERIODO = '20202' THEN 2020
WHEN PERIODO = '20213' THEN 2021
ELSE ''
END AS 'PERIODO_AÑO'
FROM [SaberProSource].[dbo].[SaberProSource]
EXCEPT
SELECT *
FROM [SaberProDWH].[dbo].[PERIODO]</property>
